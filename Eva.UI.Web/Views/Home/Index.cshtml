<article class="destaques right mgn-right-10px mgn-top-20px">
    <section class="noticia-urgente">
        <a class="fui-time" href="@Url.Action("Ler", "Noticia", new { Area = "", slugNoticia = Model.Noticias.Urgente.Slug, slugCategoria = Model.Noticias.Urgente.Categoria.Slug, slugLocal = Model.Noticias.Urgente.Local.Slug })" title="@Model.Noticias.Urgente.Titulo" style="background-color:#E74B3B;">
            @Model.Noticias.Urgente.Titulo
        </a>
        <p class="fui-time" style="color:#E74B3B;">@Model.Noticias.Urgente.Data.ToString("dd/MM/yyyy HH:mm")</p>
        <p class="fui-location" style="color:#E74B3B;">@Model.Noticias.Urgente.Local.Nome</p>
    </section>
    <section class="destaques-atual">
        <h2 class="titulo fui-menu">DESTAQUES</h2>
        <ul class="slideshow">
            @foreach (var noticia in Model.Noticias.Destaques)
            {
                <li>
                    <a href="@Url.Action("Ler", "Noticia", new { Area = "", slugNoticia=noticia.Slug ,slugCategoria = noticia.Categoria.Slug, slugLocal = noticia.Local.Slug })" title="@noticia.Categoria.Nome">
                        <p class="categ" style="background-color:@noticia.Categoria.CorFundo;color:@noticia.Categoria.CorTexto">@noticia.Categoria.Nome</p>
                        <figure>
                            @{
                var arquivo = noticia.Arquivos.FirstOrDefault() ?? new Arquivo();
                            }
                            <img src="@Foto.Placehold(arquivo.Nome, "500x282", "Noticia" , "500x282_" )" width="500" height="282" />
                        </figure>
                        <p class="hora fui-time left" style="color:@noticia.Categoria.CorFundo;">@noticia.Data.ToShortTimeString()</p>
                        <p class="local fui-location right" style="color:@noticia.Categoria.CorFundo;">@noticia.Local.Nome</p>
                        <h2 class="desc" style="color:@noticia.Categoria.CorFundo;">@noticia.Titulo</h2>
                    </a>
                </li>
            }
        </ul>
        <ul class="miniatura">
            @foreach (var noticia in Model.Noticias.Destaques)
            {
                <li>
                    <a href="#" title="">
                        <span class="molde"></span>
                        <figure>
                            <span style="background-color: @noticia.Categoria.CorFundo;"></span>
                            @{
                var arquivo = noticia.Arquivos.FirstOrDefault() ?? new Arquivo();
                            }
                            <img src="@Foto.Placehold(arquivo.Nome, " 150x85", "Noticia" , "150x85_" )" width="70" height="39" />
                        </figure>
                    </a>
                </li>
            }
        </ul>
    </section>
